/*
** Bootstrap over-rides
*/
.mailist-box .form-control::placeholder {
    color: #333;
    opacity: 1;
}
.search-form .form-control::placeholder {
    color: #000;
    opacity: 1;
}
.mailinglist-input.form-control:focus {
    outline: 0;
    box-shadow:none;
    color: #333;
}
.form-control.search-text:focus{
    outline: 0;
    box-shadow:none;
    color: #333;
}
input:focus::-webkit-input-placeholder {
    color: transparent;
}
input:focus:-moz-placeholder {
    color: transparent;
}
input:focus::-moz-placeholder {
    color: transparent;
}


html, body {
  height: 100%;
}

body {
  display: flex;
  flex-direction: column;
}
.page-content {
  flex: 1 0 auto;
}
footer {
  flex-shrink: 0;
}

body {
    font-family: 'Poppins', sans-serif;
    color: #[body_text_color];
    line-height: 1.5;
    font-size: [base_font_size]px;
}
h1 {
    font-size: [heading_base_size]px;
}

.titles {
    margin: 0 0 20px;
