}
#desk_categories .navbar-nav>li>.dropdown-menu li {
    display: inline-block;
    width: 100%;
}
.navbar-wrapper {
    padding: 30px 0;
}
.navbar.navbar-inverse {
    margin-bottom: 0;
    border: none;
    background-color: transparent;
    text-align: right;
    min-height: auto;
}
.navbar-nav {
    float: none;
    margin: 0;
    text-align: right;
}
.navbar-nav li {
    float: none;
    display: inline-block;
}
.navbar-inverse .navbar-nav li a {
    color: #[navigation-text-color];
    font-weight: 400;
    text-transform: capitalize;
    font-size: [navigation-font-size]px;
    line-height: 18px;
    min-height: 50px;
    padding: 0 15px;
}
.navbar-inverse .navbar-nav li a:hover, 
.navbar-inverse .navbar-nav li a:focus {
    color: #[navigation-text-hover-color];
    background-color: rgba(255,255,255,0.2);
