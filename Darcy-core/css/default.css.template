.form-control {
    color: #000;
    font-family: 'Poppins', sans-serif;
    font-size: 13px;
    height: 36px;
    padding: 7px 12px;
}
.form-control.search-text {
     border:1px solid #ececec;
}

.btn {
    font-size: 12px;
    padding: 8px 12px;
    font-weight: 400;
    text-transform: uppercase;
}
.btn-default,
.btn-primary:focus, 
.btn-primary:active:focus {
    border-color: #[primary-btn-border];
    background-color: #[primary-btn-bg];
    color: #[primary-btn-text];
}
.btn-default:hover,
.btn-default:focus,
.btn-default:hover:focus,
.btn-default.focus {
    color: #[primary-btn-text-hover];
    background-color: #[primary-btn-bg-hover];
    border-color: #[primary-btn-border-hover];
}
.btn-default.btn-inverse,
.btn-primary {
    color: #[secondary-btn-text];
    background-color: #[secondary-btn-bg];
    border-color: #[secondary-btn-border];
}
.btn-default.btn-inverse:hover,
.btn-primary:hover {
    color: #[secondary-btn-text-hover];
    background-color: #[secondary-btn-bg-hover];
    border-color: #[secondary-btn-border-hover];
    opacity: 0.8;
}

#desk_categories .navbar-nav {
    text-align: left !important;
}
#desk_categories .navbar-nav > li {
    display: inline-block !important;
    width: 100%;
}
#desk_categories .navbar-nav >li > a {
    color: #5f5f5f;
    padding: 15px 0;
    font-weight: 500;
    font-size: 16px;
}
#desk_categories .navbar-nav >li > a:hover {
    background: transparent;
    text-decoration: underline;
}

#desk_categories .navbar-nav>li>.dropdown-menu {
    margin-top: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    float: none;
    left: 170px;
    padding: 0;
