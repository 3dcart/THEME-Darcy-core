}

.homepage-slider .flexslider .slides img {
    width: 100%;
}


.view-cart #leftBar {
    display: none;
}
.view-cart .container.full-content .col-md-9 {
    width: 100%;
}

.checkout-1 .footer-social .col-md-6 {
    width: 100%;
}
.checkout-1 .footer-social ul {
    margin: 0;
    padding: 0;
